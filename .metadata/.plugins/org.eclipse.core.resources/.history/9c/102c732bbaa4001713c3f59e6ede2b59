fo = open("foo.txt", "r+b")
str = fo.readline();
print (str)
fo.write(str)

# Check current position
position = fo.tell();
print ("Current file position : ", position)

# Reposition pointer at the beginning once again
position = fo.seek(50, 1);
gg = fo.read();
print (str)
fo.write(str)
# Close opend file
fo.close()